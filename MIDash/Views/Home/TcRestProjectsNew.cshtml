@model MIDashboard.Models.Projects

@{
    ViewBag.Title = "TcRestProjectsNew";    
}

<h2>Team City Projects</h2>

<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Count)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Count)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Href)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Href)
        </dd>

    </dl>
</div>
<table class="table">
    <tr>
        <th>
           ProjectId
        </th>
        <th>
            Parent ProjectId
        </th>
        <th>
            Name
        </th>
        <th>
            Description
        </th>
        <th>
            Href
        </th>
        <th>
            WebUrl
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Project)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParentProjectId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Href)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WebUrl)
            </td>
            
        </tr>
    }
    </table>